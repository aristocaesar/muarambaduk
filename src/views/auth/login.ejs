<div class="flex justify-center py-8 md:py-24">
    <div class="container flex flex-col items-center justify-center text-cod-gray-950">
        <% if(error.length != 0) { %>
        <div class="w-full lg:w-6/12 border border-cod-gray-300 py-3 text-cod-gray-800 font-semibold rounded-sm">
            <div class="flex border-b border-cod-gray-300 px-3 space-x-2 pb-3">
                <svg width="25px" height="25px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M12 17V11" stroke="#333333" stroke-width="1.5" stroke-linecap="round"/>
                    <circle cx="1" cy="1" r="1" transform="matrix(1 0 0 -1 11 9)" fill="#333333"/>
                    <path d="M7 3.33782C8.47087 2.48697 10.1786 2 12 2C17.5228 2 22 6.47715 22 12C22 17.5228 17.5228 22 12 22C6.47715 22 2 17.5228 2 12C2 10.1786 2.48697 8.47087 3.33782 7" stroke="#333333" stroke-width="1.5" stroke-linecap="round"/>
                </svg>
                <span>Informasi</span>
            </div>
            <p class="px-3 pt-3 text-sm md:text-base font-normal"><%= error %></p>
        </div>
        <% } %>
        <img src="<%= locale %>static/image/muarambaduk-masuk.webp" alt="muarambaduk-masuk" class="w-[20rem] lg:w-[28rem] mt-8">
        <h2 class="text-[1rem] md:text-[1.1rem] font-semibold text-center mb-8">Harap masuk untuk memesan Tiket atau Paket Camping</h2>
        <a href="<%= locale %>auth/google" class="mb-16 text-[0.9rem] flex items-center space-x-4 font-semibold border border-cod-gray-300 rounded-full px-8 py-2 bg-white-50 hover:drop-shadow-md">
            <img src="<%= locale %>static/image/svg/google.svg" alt="logo-google">
            <span>Masuk Dengan Google</span>
        </a> 
    </div>
</div>