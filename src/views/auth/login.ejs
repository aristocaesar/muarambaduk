<div class="flex justify-center py-8 md:py-24">
    <div class="container flex flex-col items-center justify-center text-cod-gray-950">
        <% if(error.length != 0) { %>
        <div class="w-full lg:w-6/12 bg-denim-500 px-5 py-3 text-white-50 font-semibold rounded-sm">
            <%= error %>
        </div>
        <% } %>
        <img src="<%= locale %>static/image/muarambaduk-masuk.webp" alt="muarambaduk-masuk" class="w-[20rem] lg:w-[28rem] mt-8">
        <h2 class="text-[1rem] md:text-[1.1rem] font-semibold text-center mb-8">Harap masuk untuk memesan Tiket atau Paket Camping</h2>
        <a href="<%= locale %>auth/google" class="mb-16 text-[0.9rem] flex items-center space-x-4 font-semibold border border-cod-gray-300 rounded-full px-8 py-2 bg-white-50 hover:drop-shadow-md">
            <img src="<%= locale %>static/image/svg/google.svg" alt="logo-google">
            <span>Masuk Dengan Google</span>
        </a> 
    </div>
</div>