            <div class="flex flex-col sm:flex-row sm:justify-between space-y-5 sm:space-y-0">
                <div class="space-y-1 text-sm md:text-base">
                    <p class="font-semibold mb-5">Nama Pemesan</p>
                    <p class="font-normal"><%= history.user.fullname %></p>
                    <p class="font-normal"><%= history.user.email %></p>
                </div>
                <div class="space-y-1 text-sm md:text-base">
                    <p class="font-semibold mb-5 sm:text-right">Tanggal Kunjungan</p>
                    <p class="font-normal sm:text-right"><%= history.date_formated %></p>
                    <p class="font-normal sm:text-right"><%= history.date_types == 'normal' ? 'Hari Biasa' : 'Hari Akhir Pekan / Libur' %> - <%= history.camping ? 'Berkemah' : 'Tidak Berkemah' %></p>
