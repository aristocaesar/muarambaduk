                        <div class="flex">
                            <img src="<%= history.packages[index].image %>" alt="packages" class="w-[70px] md:w-[100px] rounded-sm object-cover">
                            <div class="ml-3 flex flex-col space-y-1">
                                <p><%= history.packages[index].title %></p>
                                <p class="text-sm text-cod-gray-300">Quantity : <%= history.packages[index].quantity %></p>
                            </div>
                        </div>
                        <p><%= history.packages[index].price_formated %></p>
                    </div>
                <% } %>
            </div>
            <% } %>

            <div class="flex justify-between mt-16 border-b border-cod-gray-300 pb-5 mb-5 text-sm md:text-base">
